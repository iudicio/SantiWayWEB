# Broker (RabbitMQ)
CELERY_BROKER_URL=amqp://celery:celerypassword@rabbitmq:5672//

# Имя консьюмера
CELERY_CONSUMER_NAME=Vendor

# Имя задачи, которую обрабатывает сервис
CELERY_C_TASK_NAME=vendor

# Очередь, которую слушает этот сервис
CELERY_C_QUEUE_NAME=vendor_queue

# ESWriter (Elasticsearch) - основная задача для отправки в ES
CELERY_P_TASK_NAME=esWriter

# Очередь для отправки в Elasticsearch
CELERY_P_QUEUE_NAME=esWriter_queue

# CHWriter (ClickHouse) - параллельная задача для отправки в CH
CELERY_CH_TASK_NAME=chWriter

# Очередь для отправки в ClickHouse
CELERY_CH_QUEUE_NAME=chWriter_queue

# Уровень логов
CELERY_LOG_LEVEL=INFO
